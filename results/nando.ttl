    rdfs:label "短腸症"@ja ;
    terms:identifier "2200943"^^xsd:string ;
    :has_notification_number "22"^^xsd:string ;
    rdfs:isDefinedBy <https://www.shouman.jp/archives/doc/doc_12_13_037_01.pdf> ;
