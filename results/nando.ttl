### 指定難病 167
:1200644
    rdf:type owl:Class ;
    rdfs:subClassOf :1100003 ;
    rdfs:label "マルファン症候群"@ja ;
    terms:identifier "1200644"^^xsd:string ;
    :has_notification_number "167"^^xsd:string ;
